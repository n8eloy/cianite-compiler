@cep(18, "type 'Integer' not recognized", "Invalid type 'Integer'", "");

class Teste
    var Int n;
	
    func get -> Int {
        return self.n;
    }
	
    func set: Int n {
        self.n = n;
    }
end


class Program
    func run {
        var Teste t;
        var Integer a;
        t = Teste.new;
        a = In.readInt;
        t.set: a;
        Out.println: t.get;
    }
end
